{
  "model": {
    "type": "minecraft:condition",
    "property": "minecraft:has_component",
    "component": "minecraft:item_name",
    "on_true": {
      "type": "minecraft:select",
      "property": "minecraft:component",
      "component": "minecraft:custom_name",
      "cases": [
        {
          "when": "Green LightSaber",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/weapons/netherite_sword/lightsabers/saberlight"
          }
        },
        {
          "when": "Purple LightSaber",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/weapons/netherite_sword/lightsabers/bamfsaber"
          }
        },
        {
          "when": "Red LightSaber",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/weapons/netherite_sword/lightsabers/darthsaber"
          }
        },
        {
          "when": "Blue LightSaber",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/weapons/netherite_sword/lightsabers/skysaber"
          }
        },
        {
          "when": "Infinity Blade",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/weapons/netherite_sword/customswords/infinity_blade_3d"
          }
        },
        {
          "when": "Modern Infinity Blade",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/weapons/netherite_sword/customswords/modern_infinity_blade_3d"
          }
        },
        {
          "when": "Shroom Sword",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/weapons/shroom_set/shroom_sword"
          }
        },
        {
          "when": "Orphan Obliterator",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/weapons/netherite_sword/customswords/orphan_obliterator"
          }
        },
        {
          "when": "Command Staff",
          "model": {
            "type": "minecraft:select",
            "property": "minecraft:component",
            "component": "minecraft:base_color",
            "cases": [
              {
                "when": "black",
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/weapons/misc/command_staff"
                }
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "minecraft:item/netherite_sword"
            }
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/netherite_sword"
      }
    },
    "on_false": {
      "type": "minecraft:model",
      "model": "minecraft:item/netherite_sword"
    }
  }
}